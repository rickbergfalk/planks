<div
  id="test-container"
>
  
      
  <div
    style="width: 800px; height: 500px; position: relative;"
  >
    
        
    <plank-sidebar-provider
      class="group/sidebar-wrapper has-data-[variant=inset]:bg-sidebar flex min-h-svh w-full"
      data-slot="sidebar-wrapper"
      style="--sidebar-width: 16rem; --sidebar-width-icon: 3rem;"
    >
      
          
      <plank-sidebar
        class="bg-sidebar text-sidebar-foreground flex h-full w-(--sidebar-width) flex-col"
        collapsible="none"
        data-collapsible=""
        data-mobile="true"
        data-side="left"
        data-slot="sidebar"
        data-state="expanded"
        data-variant="sidebar"
      >
        
            
        <plank-sidebar-header
          class="flex flex-col gap-2 p-2"
          data-sidebar="header"
          data-slot="sidebar-header"
        >
          
              
          <span
